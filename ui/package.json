{
  "name": "decapod",
  "version": "1.2.0",
  "description": "Decapod",
  "watch": {
    "compile-less": {
      "patterns": [
        "app/styles"
      ],
      "extensions": "less"
    }
  },
  "scripts": {
    "watch": "npm-watch",
    "start": "tsc && concurrently \"tsc -w\" \"lite-server\" \"npm run watch\"",
    "compile-less": "lessc --verbose app/styles/styles.less app/styles/styles.css",
    "docker-build": "docker build -t ng2-quickstart .",
    "docker": "npm run docker-build && docker run -it --rm -p 3000:3000 -p 3001:3001 ng2-quickstart",
    "pree2e": "npm run webdriver:update",
    "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "tsc && karma start karma.conf.js --single-run",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "webdriver:update": "webdriver-manager update",
    "build": "gulp"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "~4.0.0",
    "@angular/compiler": "~4.0.0",
    "@angular/core": "~4.0.0",
    "@angular/forms": "~4.0.0",
    "@angular/http": "~4.0.0",
    "@angular/platform-browser": "~4.0.0",
    "@angular/platform-browser-dynamic": "~4.0.0",
    "@angular/router": "~4.0.0",
    "angular-in-memory-web-api": "~0.3.0",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "file-saver": "^1.3.3",
    "format-json": "^1.0.3",
    "jquery": "^3.1.0",
    "js-cookie": "^2.1.3",
    "js-data": "3.0.0-rc.4",
    "js-data-http": "3.0.0-rc.2",
    "lodash": "^4.16.4",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.1",
    "systemjs": "0.19.40",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@types/bootstrap": "^3.3.32",
    "@types/jasmine": "^2.5.38",
    "@types/js-cookie": "^2.0.28",
    "@types/jquery": "^2.0.33",
    "@types/lodash": "^4.14.44",
    "@types/node": "^6.0.46",
    "@types/selenium-webdriver": "^2.53.34",
    "autoprefixer": "^6.5.0",
    "canonical-path": "0.0.2",
    "concurrently": "^3.1.0",
    "cssnano": "^3.7.6",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-google-webfonts": "0.0.14",
    "gulp-less": "^3.1.0",
    "gulp-minify": "0.0.14",
    "gulp-postcss": "^6.2.0",
    "gulp-replace": "^0.5.4",
    "gulp-sourcemaps": "^1.9.1",
    "gulp-typescript": "^3.1.3",
    "http-server": "^0.9.0",
    "jasmine-core": "~2.4.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-htmlfile-reporter": "^0.3.4",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "less": "^2.7.1",
